						<div id="carouselExample" class="carousel">
							<div class="carousel-inner">
								<% bannerData.forEach(function(item, index) { %>
									<div class="carousel-item <% if (index === 0) { %>active<% } %>">
										<img src="/images/uploads/<%= item.bannerImage %>" class="d-block w-100" alt="Slide <%= index + 1 %>">
										<div class="carousel-caption">
											<div class="caption-container">
												<h1 class="text-light bannerTitle">
													<%= item.mainHeading %>
												</h1>
												<h1 class="text-light bannerDescriptionbanner">
													<%= item.description %>
												</h1>
											</div>
										</div>
									</div>
									<% }) %>
							</div>
						
							<button class="carousel-control-prev carousel-button-style" onclick="changeSlide(-1)">&lt;</button>
							<button class="carousel-control-next carousel-button-style" onclick="changeSlide(1)">&gt;</button>
						</div>

						<!-- /BANNER -->

